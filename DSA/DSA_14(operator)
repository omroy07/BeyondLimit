#include <iostream>
using namespace std;
int main() {
    int a = 11, b = 5;
    cout << "Addition: " << a + b << endl;
    cout << "Subtraction: " << a - b << endl;
    cout << "Multiplication: " << a * b << endl;
    cout << "Division: " << a / b << endl;//Quotient
    cout << "Modulus: " << a % b << endl;//Remainder
    return 0;
}
